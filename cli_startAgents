set timeout 120
spawn ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null {{serverUser}}\@{{host}} -p {{serverPort}}
expect "root@ProVM:~#"
send "cd /opt/adva/aos/\r"
expect "root@ProVM:/opt/adva/aos#"
send "./nova start\r"
expect "root@ProVM:/opt/adva/aos#"
send "./neutron start\r"
expect "root@ProVM:/opt/adva/aos#"
sleep 3
send "logout\r"
expect "closed."
